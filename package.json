		"axios": "^1.6.2",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.0",
		"node-fetch": "^3.3.2",
		"path": "^0.12.7",
		"phaser": "^3.70.0",
		"prettier": "^3.1.1",
